# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/_/settings/api

# Public (safe for client-side)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ...

# Private (server-side only - NEVER expose to client)
SUPABASE_SERVICE_ROLE_KEY=eyJ...

# Oracle Configuration
ORACLE_PRIVATE_KEY=0x...

# AI Moderation (pick one)
NVIDIA_API_KEY=nvapi-...          # Free Kimi K2.5 via NVIDIA NIM
# OPENROUTER_API_KEY=sk-or-...    # Kimi K2.5 via OpenRouter ($0.45/M)
# ANTHROPIC_API_KEY=sk-ant-...    # Claude Haiku (paid)

# Feature Flags (all default to false)
ENABLE_SUPABASE_UPLOAD=false
ENABLE_SUPABASE_ENTRIES=false
ORACLE_USE_SUPABASE=false
USE_NEW_SUBMIT_FLOW=false
